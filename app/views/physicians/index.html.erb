<p style="color: green"><%= notice %></p>
<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Full Name</th>
      <th scope="col">Edit</th>
      <th scope="col">Show</th>
      <th scope="col">Destroy</th>
    </tr>
  </thead>
  <tbody>
    <% @physicians.each do |physician| %>
      <tr>
        <td><%= physician.name %></td>
        <td><%= physician.last_name %></td>
        <td><%= physician.name %> <%= physician.last_name %></td>
        <td><%= link_to "Edit", edit_physician_path(physician) %></td>
        <td><%= link_to "Show", physician_path(physician) %></td>
        <td><%= link_to "Destroy", physician, data: { turbo_confirm: "Are you sure you want to delete this user", turbo_method: :delete } %></td>
      </tr>
    <% end %>  
  </tbody>
</table>

<%= link_to "New physician", new_physician_path %>
